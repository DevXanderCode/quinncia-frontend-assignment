{
    "$schema": "http://json.schemastore.org/prettierrc",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "endOfLine": "auto",
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false,
    "quoteProps": "as-needed",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "proseWrap": "always",
    "arrowParens": "always",
    "htmlWhitespaceSensitivity": "strict",
    "insertPragma": false,
    "jsxSingleQuote": false,
    "requirePragma": false
  }